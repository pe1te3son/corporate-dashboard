<main class="mdl-layout__content mdl-color--grey-100">
  <div class="mdl-grid dashboard-content">
    <div class="dashboard-charts mdl-color--white mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-grid">
      <p>
        {{numberOfEmployees}}
      </p>
    </div>
    <div class="dashboard-graphs mdl-shadow--2dp mdl-color--white mdl-cell mdl-cell--12-col">
      {{#if dataArray}}
        {{geo-graph
          class="geo-graph-home-page"
          apiKey=apiKey
          data=dataArray
        }}
      {{/if}}
    </div>
  </div>
</main>

{{svg-temporaly}}
